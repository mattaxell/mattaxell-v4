  src: url("../fonts/basis-grotesque-regular-pro.eot");
  src: url("../fonts/basis-grotesque-regular-pro.eot?#iefix") format("embedded-opentype"), url("../fonts/basis-grotesque-regular-pro.woff") format("woff"), url("../fonts/basis-grotesque-regular-pro.woff2") format("woff2"), url("../fonts/basis-grotesque-regular-pro.ttf") format("truetype");
  font-weight: normal;
  font-style: normal; }

body {
  color: #2b2b2b;
  font-family: 'basis-grotesque-regular-pro', -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
  font-size: 20px;
  line-height: 1.333; }
  @media (min-width: 768px) {
    body {
      font-size: 24px; } }
  @media (min-width: 1024px) {
    body {
      font-size: 28px; } }
  @media (min-width: 1200px) {
    body {
      font-size: 32px; } }
  body.dark {
    background: #2b2b2b;
    color: #e4e4e4; }
    body.dark .footer {
      background: rgba(255, 255, 255, 0.05); }
    body.dark a, body.dark a:visited {
      color: #00fdd2; }

a {
  color: #0000ee; }
  a:visited {
    color: #0000ee; }

.wrap {
  max-width: 1200px;
  margin-left: auto;
  margin-right: auto;
  padding-left: 32px;
  padding-right: 32px; }
  @media (min-width: 667px) {
    .wrap {
      width: 80%;
      padding-left: 0;
      padding-right: 0; } }
  @media (min-width: 1300px) {
    .wrap {
      min-width: 1200px; } }

.insulate {
  padding-top: 10%;
  padding-bottom: 10%; }
  @media (min-width: 1600px) {
    .insulate {
      padding-top: 160px;
      padding-bottom: 160px; } }
  .insulate + .insulate {
    padding-top: 0; }

.hero .wrap {
  padding: 0; }
  @media (min-width: 667px) {
    .hero .wrap {
      display: flex;
      align-items: center; } }

.hero__content {
  padding: 64px 24px; }
  @media (min-width: 667px) {
    .hero__content {
      position: relative;
      width: 50%;
      flex: 0 0 auto; } }
  .hero__content h1 {
    margin: 0; }
  .hero__content a {
    display: block;
    margin-top: 1em; }

.hero img {
  display: block; }
  @media (min-width: 667px) {
    .hero img {
      width: 65%;
      margin-left: -15%;
      flex: 0 0 auto; } }

.tag {
  display: inline-flex;
  align-items: center;
  color: #767676;
  font-size: 10px;
  letter-spacing: 2px;
  text-transform: uppercase;
  line-height: 1; }
  @media (min-width: 768px) {
    .tag {
      font-size: 11px;
      letter-spacing: 4px; } }
  .tag:before {
    content: "";
    display: block;
    width: 6px;
    height: 6px;
    margin-right: 12px;
    border-radius: 100%;
    background: #ff2f63; }
  .tag a {
    text-decoration: none; }
