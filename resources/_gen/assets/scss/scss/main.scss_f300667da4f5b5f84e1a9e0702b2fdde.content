h1,
h2,
h3,
h4,
h5,
h6,
p {
  margin-top: 0;
  font-weight: 400; }

h1 {
  line-height: 1.333; }
  h1 {
    font-size: 24px; }
    @media screen and (min-width: 320px) {
      h1 {
        font-size: calc(24px + 24 * ((100vw - 320px) / 880)); } }
    @media screen and (min-width: 1200px) {
      h1 {
        font-size: 48px; } }
  @media (min-width: 1024px) {
    h1 {
      line-height: 1.2; } }

a svg {
  width: 16px;
  height: 16px; }
  @media (min-width: 768px) {
    a svg {
      width: 20px;
      height: 20px; } }
  @media (min-width: 1024px) {
    a svg {
      width: 24px;
      height: 24px; } }

a[data-label] {
  position: relative; }
  a[data-label]:hover:after {
    opacity: 1;
    pointer-events: auto; }
  a[data-label]:after {
    content: attr(data-label);
    position: absolute;
    bottom: calc(100% + 16px);
    left: 50%;
    transform: translateX(-50%);
    opacity: 0;
    display: block;
    padding: 0.833em;
    background: rgba(0, 0, 0, 0.85);
    border-radius: 3px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
    color: #ffffff;
    font-size: 13px;
    line-height: 1em;
    text-decoration: none;
    text-transform: none;
    letter-spacing: 0;
    white-space: nowrap;
    pointer-events: none;
    transition: all 300ms ease; }

.header {
  padding: 40px 0; }
  @media (min-width: 1024px) {
    .header {
      padding: 60px 0; } }
  .header .wrap {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    justify-items: center; }
  .header .logo-anchor {
    grid-column: 2 / 3; }
    .header .logo-anchor:visited {
      color: inherit !important; }
  .header .logo {
    display: block;
    width: 57px;
    height: 32px; }
    @media (min-width: 1024px) {
      .header .logo {
        width: 80px;
        height: 45px; } }
  .header .tag {
    justify-self: end; }

.footer {
  background: #eeeeee; }
  .footer nav a {
    display: block; }
    @media (min-width: 1200px) {
      .footer nav a {
        margin: 0.5em 0; } }

.portfolio .wrap {
  overflow: hidden;
  padding-right: 0;
  padding-left: 0; }
  @media (min-width: 1024px) {
    .portfolio .wrap {
      padding-bottom: 120px; } }

@media (min-width: 1024px) {
  .portfolio .project {
    margin-bottom: 60px; }
    .portfolio .project:nth-child(even) {
      transform: translateY(120px); } }

.portfolio .project__details {
  padding: 32px 24px 48px;
  line-height: 1; }
  @media (min-width: 1024px) {
    .portfolio .project__details {
      padding-top: 32px;
      padding-right: 0;
      padding-bottom: 0;
      padding-left: 0; } }
  .portfolio .project__details h4 {
    margin: 0; }
    .portfolio .project__details h4 a {
      color: inherit !important;
      text-decoration: none; }

@font-face {
